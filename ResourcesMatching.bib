  year = {2009},
  keywords = {construction simulation,methodologies,model,waste},
  pages = {448--457},
  file = {/Users/daniel/CloudStation/Literature/Zotero/Storage/Resources Matching/Simulation Approach to Evaluating Cost Efficiency of Selective Demolition Practices - Case of Hong Kong’s Kai Tak Airport Demolition.pdf}
}

