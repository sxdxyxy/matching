  author = {Li, Y. and Lu, Y. and Li, D. and Ma, L.},
  month = jun,
  year = {2015},
  pages = {04015044},
  file = {/Users/daniel/CloudStation/Literature/Zotero/Storage/Resources Matching/Metanetwork Analysis for Project Task Assignment.pdf}
